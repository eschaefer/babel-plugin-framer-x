{
  "name": "babel-plugin-framer-x",
  "version": "1.0.0",
  "description": "Remove Framer X related code from your production bundles.",
  "main": "lib/index.js",
  "repository": "https://github.com/eschaefer/babel-plugin-framer-x",
  "author": "Eric Schaefer",
  "license": "MIT",
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "style": "prettier --write src/**/*.js",
    "build": "mkdir ./lib && cp src/* ./lib/",
    "test": "jest -u",
    "test:watch": "npm run test -- --watch",
    "prepublish": "npm run clean && npm run build",
    "release": "np --no-yarn"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-syntax-flow": "^7.0.0",
    "@babel/plugin-syntax-typescript": "^7.0.0",
    "babel-core": "^7.0.0-bridge",
    "babel-jest": "^23.6.0",
    "jest": "^23.6.0",
    "np": "^3.0.4",
    "rimraf": "^2.6.2"
  },
  "keywords": [
    "babel-plugin",
    "react",
    "framer",
    "framer x",
    "design system",
    "sketch"
  ],
  "dependencies": {
    "@babel/plugin-syntax-class-properties": "^7.0.0"
  }
}
